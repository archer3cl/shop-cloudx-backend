{
  "name": "shop-cloudx-backend",
  "version": "1.0.0",
  "description": "Product Service",
  "main": "store/serverless.ts",
  "scripts": {
    "task6:print": "sls print --service=import-service",
    "task6:local:catalog-batch-process": "sls invoke local --service=import-service -f catalogItemsQueueWorker --path src/functions/catalogBatchProcess/mock.json",
    "cloudformation:setup": "sls deploy",
    "cloudformation:remove": "sls remove",
    "dynamodb:remove": "sls remove --service=dynamodb",
    "db:seed": "./dynamodb/resources/dynamodb-seed-setup.sh",
    "import-service:setup": "sls deploy --service=import-service",
    "import-service:remove": "sls remove --service=import-service",
    "test": "jest --config=jest.config.ts",
    "task3:local:getProductsById": "sls invoke local --service=store -f getProductsById --path src/functions/products/getProductsById/mock.json",
    "task3:local:getProductsList": "sls invoke local --service=store -f getProductsList --path src/functions/products/getProductsList/mock.json",
    "task3:local:createProduct": "sls invoke local --service=store -f createProduct --path src/functions/products/createProduct/mock.json",
    "task5:local:import-service": "sls invoke local --service=import-service -f importProductsFile --path src/functions/importProductsFile/mock.json",
    "task5:local:parcer": "sls invoke local --service=import-service -f importFileParser --path src/functions/importFileParser/mock.json",
    "apidocs:build": "npx redoc-cli --output index.html build openapi.yaml"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.288.0",
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/client-sns": "^3.293.0",
    "@aws-sdk/client-sqs": "^3.290.0",
    "@aws-sdk/lib-dynamodb": "^3.288.0",
    "@aws-sdk/s3-request-presigner": "^3.282.0",
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.2.5",
    "@middy/http-error-handler": "^4.2.6",
    "@middy/http-event-normalizer": "^4.2.6",
    "@middy/http-header-normalizer": "^4.2.6",
    "@middy/http-json-body-parser": "^3.4.0",
    "@middy/input-output-logger": "^4.2.6",
    "csv-parser": "^3.0.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@aws-sdk/util-stream-node": "^3.282.0",
    "@serverless/compose": "^1.3.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "aws-sdk-client-mock": "^2.1.0",
    "aws-sdk-client-mock-jest": "^2.1.0",
    "esbuild": "^0.14.11",
    "jest": "^29.4.3",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-lift": "^1.25.1",
    "serverless-s3-cleaner": "^2.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
